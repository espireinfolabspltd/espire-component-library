{
  "name": "espire-component-library",
  "version": "0.0.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc",
    "main": "dist/main.tsx",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./src",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "if [%NODE_ENV%] NEQ [production] npm install --legacy-peer-deps"
  },
  "lint-staged": {
    "src/**/*": [
      "npm run format"
    ]
  },
  "files": [
    "dist/"
  ],

  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/material-icons": "^5.0.11",
    "@fontsource/roboto": "^5.0.8",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.7"
  },

  "exports": {
    "/erf_appbar" : "./src/components/erf_appbar/aapbar.tsx"
  },

  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.8",
    "@storybook/addon-interactions": "^7.6.8",
    "@storybook/addon-links": "^7.6.8",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/addon-themes": "^7.6.8",
    "@storybook/blocks": "^7.6.8",
    "@storybook/react": "^7.6.8",
    "@storybook/react-vite": "^7.6.8",
    "@storybook/test": "^7.6.8",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.2.2",
    "storybook": "^7.6.8",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
